{
  "name": "epoc-mobile",
  "version": "0.7.0",
  "author": "Inria Learning Lab",
  "homepage": "https://learninglab.inria.fr",
  "scripts": {
    "dev": "ionic serve --external --host=$(hostname)",
    "build": "ng build",
    "build-preview": "ng build --configuration=preview --output-path=dist",
    "android": "npm run resources && ionic capacitor run android --prod --open",
    "android-zrr": "npm run resources-zrr && ionic capacitor run android --prod --open --mode=inria",
    "android-dev": "npm run resources && ionic build && ionic capacitor run android --open --livereload-url=http://$(hostname):8100",
    "ios": "npm run resources && ionic capacitor run ios --open --prod",
    "ios-zrr": "npm run resources-zrr && ionic capacitor run ios --open --prod --mode=inria",
    "ios-dev": "npm run resources && ionic build && ionic capacitor run ios --open --livereload-url=http://$(hostname):8100",
    "e2e-local": "npx wdio run ./e2e/wdio.conf.local.js || true",
    "e2e": "npx wdio run ./e2e/wdio.conf.bs.js || true",
    "resources": "rm -rf resources/* && cp -R resources-common/* resources/ && cordova-res --skip-config --copy",
    "resources-zrr": "rm -rf resources/* && cp -R resources-zrr/* resources/ && cordova-res --skip-config --copy"
  },
  "postinstall": "jetifier",
  "private": true,
  "dependencies": {
